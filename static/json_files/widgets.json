   "MSSQL_Table_Creation": [
        
      "CREATE TABLE auth(id INTEGER,computer_name TEXT,mac_address TEXT,serial_key Text,license TEXT,Installed_date TEXT,expiry_date TEXT,remaining_days INTEGER,status TEXT,last_verified TEXT,current_version TEXT ,Operating_System TEXT,Update_Status TEXT, last_Update TEXT, UR_Process TEXT,email TEXT, PRIMARY KEY( id autoincrement))",
      "CREATE TABLE  User_Action  ( id integer, Date_Time TEXT, File_Name TEXT, Source_Path TEXT, Hash_ID TEXT, Action TEXT,unique(Source_Path) FOREIGN KEY( Hash_ID ) REFERENCES  Offline_Master ( id ),PRIMARY KEY( id  AUTOINCREMENT))",
      "CREATE TABLE  Offline_Master  ( id integer, Date_Time TEXT, MD5_HASH TEXT  NOT NULL, SHA256_HASH TEXT  NOT NULL, Malicious TEXT, Threat_Name TEXT, Ignore_Action TEXT, unique(MD5_HASH),PRIMARY KEY( id  AUTOINCREMENT))",
      "CREATE TABLE  Scan_History  ( id integer, Date_Time  TEXT, Scan_Type TEXT, Scan_Status TEXT, Scanned_Count TEXT, Threat_Count TEXT, Duration TEXT, Scan_Path TEXT, unique(Date_Time),PRIMARY KEY( id  AUTOINCREMENT))",
      "CREATE TABLE  Status_Master  ( id integer, Browse_Option TEXT, Full_Scan TEXT, Custom_Scan TEXT, Real_Time_Protection TEXT, Threat_Action TEXT, Header_Msg TEXT, otp TEXT, otp_snt_time TEXT,Proxy_Status Test, PRIMARY KEY( id  AUTOINCREMENT))"
   ],
